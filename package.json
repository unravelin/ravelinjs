{
  "name": "ravelinjs",
  "version": "1.0.0",
  "description": "Ravelin's Javascript Library.",
  "main": "ravelin.js",
  "scripts": {
    "minify": "uglifyjs --support-ie8 -o ravelin.min.js ravelin.js",
    "minify-latest": "uglifyjs --support-ie8 -o ravelin.min.js ravelin.js && cp ravelin.min.js ravelin-$(node -e 'console.log(require(\"./package.json\").version)').min.js",
    "test": "mocha -C test/test.js",
    "test-cover": "nyc --reporter=html mocha -C test/test.js",
    "test-debug": "mocha --inspect-brk test/test.js",
    "test-appium": "npm run minify && wdio ./test/appium.conf.js",
    "test-crossbrowser": "npm run minify && wdio ./test/crossbrowser.conf.js",
    "test-crossbrowser-e2e": "E2E_CIPHERTEXT_FILE=./cipher.txt E2E_DEVICEID_FILE=./deviceid.txt E2E_SESSIONID_FILE=./sessionid.txt wdio ./test/crossbrowser-e2e.conf.js",
    "webpack": "webpack ./test/pages/webpack/index.js -o ./test/pages/webpack/bundle.js",
    "live-crossbrowser": "sh -u -c 'cbt_tunnels --username $CROSSBROWSERTESTING_USER --authkey $CROSSBROWSERTESTING_KEY --dir ./test/'",
    "go-serve": "goexec 'http.ListenAndServe(\":8088\", http.FileServer(http.Dir(\".\")))'",
    "go-sri": "sri -out=sri.json -hash=all ravelin.js ravelin.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unravelin/ravelinjs.git"
  },
  "author": "Ravelin (https://github.com/unravelin) <support@ravelin.com>",
  "license": "TBD",
  "bugs": {
    "url": "https://github.com/unravelin/ravelinjs/issues"
  },
  "homepage": "https://developer.ravelin.com/libraries-and-sdks/ravelinjs/",
  "devDependencies": {
    "appium": "^1.8.0",
    "appium-doctor": "^1.4.3",
    "cbt_tunnels": "^0.9.4",
    "chai": "^4.1.2",
    "circle-wait": "^1.0.2",
    "nyc": "^13.1.0",
    "request": "^2.85.0",
    "sinon": "^7.1.1",
    "uglify-js": "^2.8.29",
    "wait-port": "^0.2.2",
    "wdio-appium-service": "^0.2.3",
    "wdio-junit-reporter": "^0.4.2",
    "wdio-mocha-framework": "^0.5.13",
    "wdio-spec-reporter": "^0.1.4",
    "webdriverio": "^4.12.0",
    "webpack": "^4.8.3"
  }
}
